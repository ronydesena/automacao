      "modal-trigger#assembly-info"
    ]
  },

  "modal-trigger#assembly-options": {
    "children": ["rich-text#assembly-options", "modal-layout#assembly-options"],
    "props": {
      "blockClass": "modal-assembly-options"
    }
  },

  "modal-layout#assembly-options": {
    "children": ["assembly-options-modal"],
